/* -------------------------------- WHITE LABEL -------------------------------- */

.user-css-whitelabel {
   bottom: 50px;
   border-radius: var(--cont-rad);
   padding: var(--cont-pad-tiny);
   background: #00000099;
   box-shadow: var(--box-shadows);
   -moz-box-shadow: var(--box-shadows);
   -webkit-box-shadow: var(--box-shadows);
   opacity: .8;
   text-align: center;
}

  

.user-css-whitelabel small a {
    content: url(https://www.worldanvil.com/uploads/images/0f448d0499c3bc69c2852a37ad390f51.png);
    filter: drop-shadow(0 0 3px var(--aurora-1d));
    width: 100px;
    padding: 3px;
    display: block;
}

/* move the white label to the bottom on small screens */
@media only screen and (max-width: 1399px) { 
.user-css-whitelabel {
   bottom: -20px;
   box-shadow: none;
   -moz-box-shadow: none;
   -webkit-box-shadow: none;
    border-radius: 0;
   background: var(--aurora-nightd);
   opacity: 1;
   text-align: center;
   display: block;
   z-index:1000;
    width: 100%;
    margin-left: -20px;
    
}

.user-css-whitelabel small a {
    padding: 0;
   display: inline;
    font-size: var(--fsize-small);
}
    /* extend the page a bit so the whitelabel doesn&#039;t cover up anything */
    .user-css-extended {
        padding-bottom: 100px;
    }
} 

.user-css-whitelabel small, .user-css-whitelabel strong {
   display: block;
   font-size: 10pt;
}

.user-css-whitelabel small{
    color:white;
}

