} */


.user-css .btn-default, .user-css-extended .btn-default, .user-css .label-default, .user-css .badge-default { 
	font-weight: 600; 
	color: var(--aurora-night); 		
	background: var(--aurora-default);
}
.user-css .btn-primary, .user-css-extended .btn-primary, .user-css .label-primary, .user-css .badge-primary  { 
	background: var(--aurora-primary);
}
.user-css .btn-primary:hover, .user-css-extended .btn-primary:hover { background: var(--aurora-8);
}
.user-css .btn-success, .user-css-extended .btn-success, .user-css .label-success, .user-css .badge-success  { 
	background: var(--aurora-success) !important;
}
.user-css .btn-info, .user-css-extended .btn-info, .user-css .label-info, .user-css .badge-info { 
	background: var(--aurora-info);
}
.user-css .btn-warning, .user-css .label-warning, .user-css .badge-warning { 
	background: var(--aurora-warning);
}
.user-css .btn-danger, .user-css-extended .btn-danger, .user-css .label-danger, .user-css .badge-danger, .user-css-map-sidebar .badge-danger { 
	background: var(--aurora-danger);
}
.user-css .btn-danger:hover, .user-css-extended .btn-danger:hover{
	background: var(--aurora-9);
}
.user-css .btn-link, .user-css-extended .btn-link, .user-css .badge-link { 
	color: var(--aurora-link-text); background: var(--aurora-link);
}

.user-css .btn-default:active { 	
	background-color: var(--aurora-wt); }
.user-css .btn-primary:active { 	
	background-color: var(--aurora-8t); }
.user-css .btn-success:active { 
		background-color: var(--aurora-5t) !important; }
.user-css .btn-info:active { 
		background-color: var(--aurora-3t); }
.user-css .btn-warning:active { 
		background-color: var(--aurora-ft); }
.user-css .btn-danger:active { 
		background-color: var(--aurora-9t); }
.user-css .btn-link:active { background-color: var(--aurora-7t); }

.user-css .breadcrumb {
    padding: var(--cont-pad-sm) var(--cont-pad-med);
    margin-bottom: var(--cont-pad);
    list-style: none;
    background-color: var(--aurora-link);
    border-radius: var(--cont-rad);
	border-right: none;
	border-left: none;
    border-top: var(--border-thin) solid var(--aurora-7l);
	border-bottom: var(--border-thin) solid var(--aurora-7l);
}

.user-css .progress {
	background-color: var(--aurora-wl);
	border-radius: var(--cont-rad);
}
.user-css .progress-bar {
	background-color: var(--aurora-7);
	color: var(--aurora-wl);
}
.user-css .progress-bar-success {
	background-color: var(--aurora-success);
}
.user-css .progress-bar-info {
	background-color: var(--aurora-info);
}
.user-css .progress-bar-warning {
	background-color: var(--aurora-warning);
}
.user-css .progress-bar-danger {
	background-color: var(--aurora-danger);
}

.user-css .panel {
	background-color: var(--aurora-clear);
	color: var(--aurora-text);
	padding: 0;
	border-radius: var(--cont-rad);
	margin-top: var(--cont-pad-med);
	margin-bottom: var(--cont-pad-med);
}
.user-css .panel-group {
	background-color: var(--aurora-clear);
	color: var(--aurora-text);
}
.user-css .panel-body  {
	background-color: var(--aurora-nightt);
	color: var(--aurora-text);
	padding: var(--cont-pad-sm);
	border-radius: 0 !important;
}


.user-css .panel { border-radius: var(--cont-rad); }
.user-css-extended .panel-default {
	background-color: var(--aurora-wt);
	color: var(--aurora-text);
}
.user-css .panel-heading, .user-css-extended .panel-heading {
	background-color: var(--aurora-w);
	color: var(--aurora-night);
	border-top-right-radius: var(--cont-rad) !important;
	border-top-left-radius: var(--cont-rad) !important;
}
.user-css-extended .panel-heading h3 {
	color: var(--aurora-text); 
	letter-spacing: var(--letter-space);
	text-transform: uppercase;
	font-family: var(--font-5);
}
.user-css-extended .panel-default .panel-heading small {color: var(--aurora-text); }
.user-css .panel-footer {
	background-color: var(--aurora-default);
	color: var(--aurora-textd);
	padding: var(--cont-pad-sm);
	border-bottom-right-radius: var(--cont-rad);
	border-bottom-left-radius: var(--cont-rad);
}
.user-css .panel-default, .user-css .panel-default  {
	background-color: var(--aurora-clear);
	color: var(--aurora-textd);
}
.user-css .panel-success {
	background-color: var(--aurora-success);
	color: var(--aurora-text);
}
.user-css .panel-info {
	background-color: var(--aurora-info);
	color: var(--aurora-text);
}
.user-css .panel-warning {
	background-color: var(--aurora-warning);
	color: var(--aurora-text);
}
.user-css .panel-danger {
	background-color: var(--aurora-danger);
	color: var(--aurora-text);
}


.user-css .list-group-item:first-of-type {
	border-top-right-radius: var(--cont-rad);
	border-top-left-radius: var(--cont-rad);
	border-top: var(--border-thin) solid var(--aurora-wtb);
}
.user-css .list-group-item:last-of-type {
	border-bottom-right-radius: var(--cont-rad);
	border-bottom-left-radius: var(--cont-rad);
	border-bottom: var(--border-thin) solid var(--aurora-wtb);
}
.user-css .list-group-item {
	background-color: var(--aurora-wtb);
	color: var(--aurora-textd);
	border: 0;
}
.user-css .list-group-item.active {
	background-color: var(--aurora-nightt);
	color: var(--aurora-text);
	font-family: var(--font-5);
}
.user-css .list-group-success {
	background-color: var(--aurora-success);
	color: var(--aurora-text);
}
.user-css .list-group-info {
	background-color: var(--aurora-info);
	color: var(--aurora-text);
}
.user-css .list-group-warning {
	background-color: var(--aurora-warning);
	color: var(--aurora-text);
}
.user-css .list-group-danger {
	background-color: var(--aurora-danger);
	color: var(--aurora-text);
}

.user-css .nav-tabs, .user-css-extended .nav-tabs {
	margin-left: 5px;
	margin-right: 5px;
}
.user-css .nav-tabs li a, .user-css-extended .nav-tabs li a{
	border-top-right-radius: var(--cont-rad);
	border-top-left-radius: var(--cont-rad);
	border: var(--border-thin) solid var(--aurora-wtb);
	font-family: var(--font-5);
	background-color: var(--aurora-nightt2);
	color: var(--aurora-text);
	font-size: var(--fsize-text);
