@version        0.3.4
@description    This theme is inspired by the Finnish Foxfire legend
@author         Amy Winters-Voss (shyredfox on World Anvil, ShyRedFox#7256 on Discord) 

---- Please contact me, not Dimitris with any issues in the theme. Thanks! ;) ----

Theme sampler can be found at: https://www.worldanvil.com/w/aurora-theme-shyredfox

==/UserStyle== */



/*  ---------------------------------
AURORA THEME Variables
-------------------------------- */
:root {

    /* pallete */
    --aurora-1: #D9F96C;
    /* chartreuse */
    --aurora-1t: #D9F96C80;
	--aurora-1tb: #D9F96C99;
    --aurora-1l: #EFF9CB;
    --aurora-1d: #9CC11E;
    --aurora-2: #7BB14B;
    /* lime*/
    --aurora-2t: #7BB14B80;
    --aurora-2l: #CBF6A4;
    --aurora-2d: #599028;
    --aurora-3: #4BB179;
    /* green*/
    --aurora-3t: #4BB17980;
    --aurora-3l: #9DE2BC;
    --aurora-3d: #289057;
    --aurora-4: #00858A;
    /* teal*/
    --aurora-4t: #00858A80;
	--aurora-4tb: #00858A99;
    --aurora-4l: #79DDDF;
    --aurora-4lt: #79DDDF80;
	--aurora-4ltb: #79DDDF99;
    --aurora-4d: #016F73;
    --aurora-5: #3B8CA6;
    /* sky */
    --aurora-5t: #3B8CA680;
	--aurora-5t2: #3B8CA660;
    --aurora-5l: #59B3CF;
    --aurora-5d: #086D8C;
    --aurora-6: #00618A;
    /* cerulean */
    --aurora-6t: #00618A80;
    --aurora-6l: #0581B4;
    --aurora-6d: #004E6F;
    --aurora-7: #3517AD;
    /* blue violet*/
    --aurora-7t: #1C008A60;
    --aurora-7l: #4822E4;
    --aurora-7d: #1B0180;
    --aurora-8: #7A008A;
    /* purple*/
    --aurora-8t: #7A008A80;
    --aurora-8l: #9D02B1;
    --aurora-8d: #570162;
    --aurora-9: #B90E6B;
    /* magenta*/
    --aurora-9t: #CA003D80;
    --aurora-9l: #EF168B;
    --aurora-9d: #7B0043;
    --aurora-f: #D35E03;
    /* fox*/
    --aurora-ft: #D35E0380;
    --aurora-fl: #FC8B33;
    --aurora-fd: #9F4600;
    --aurora-night: #090649;
    /* midnight blue */
    --aurora-nightt: #09064980;
	--aurora-nightt2: #09064990;
	--aurora-nightt3: #09064950;
    --aurora-nightl: #221F69;
    /* dark grey */
    --aurora-nightd: #000;
    /* black */
    --aurora-w: #CCC;
    /* almost white */
    --aurora-wt: #CCCCCC70;
    --aurora-wtb: #FFFFFF99;
    --aurora-wl: #EEE;
    --aurora-wtl: #FFFFFF20;
    --aurora-wd: #888888;
    --aurora-clear: none;
	
	/* bootstrap/theme color names */
	--aurora-success: var(--aurora-5);
	--aurora-info: var(--aurora-3d);
	--aurora-warning: var(--aurora-fd);
	--aurora-danger: var(--aurora-9d);
	--aurora-primary: var(--aurora-8d);
	--aurora-default: var(--aurora-wtb);
	--aurora-link-text: var(--aurora-4l);
	--aurora-link: var(--aurora-7d);
	--aurora-text: var(--aurora-wl);
	--aurora-textd: var(--aurora-night);
	--aurora-text-danger: var(--aurora-9l);


    /* fonts */
    --font-1:  Shrikhand, Noto;
    --font-2: Lemon, Noto;
    --font-3: Varela Round, Noto;
    --font-4: Courgette, Noto;
	--font-5: Merienda One, Noto;
    --font-code: JetBrains Mono, Noto;

    --font-reg: 400;
    --font-bold: 800;

    --fsize-h0: 0pt;
    --fsize-h1: 60pt;
    --fsize-h2: 16pt;
    --fsize-h3: 16pt;
    --fsize-h4: 14pt;
    --fsize-h5: 14pt;
    --fsize-h6: 14pt;
    --fsize-text: 14pt;
    --fsize-big: 1.5em;
    --fsize-small: .5em;
    --fsize-med: .7em;
	--fsize-element: 50pt;
	
	--letter-space-sm-text: 0.1em;
	--letter-space: .05em;
	--letter-space-lg: .1em;

    /*  containers */
    --cont-rad: 10px;
    --sect-rad: 5px;
    --border-tiny: 1px;
    --border-thin: 2px;
    --border-medthin: 3.5px;
    --border-med: 5px;
    --border-thick: 10px;
    --border-xthick: 20px;
	--cont-pad-tiny: 5px;
    --cont-pad-sm: 10px;
    --cont-pad-med: 15px;
    --cont-pad: 20px;
	--cont-pad-lg: 40px;
    --box-shadows: 0 0 4px 3px var(--aurora-1l);
    --glow-params: 0 0 3px 3px var(--aurora-2l), inset 0 0 2px 2px var(--aurora-2l);
    --glow-params-bow: 0 0 3px 3px var(--aurora-6l), inset 0 0 2px 2px var(--aurora-6l);
		
	/* campaign containers */
	--camp-top-marg: 180px;
	--camp-marg: -15px;
	
    /* text */
    --indent: 20px;

    /* MY CONTAINERS */
    --HL-background: #FFEDB0;
    --HL-color: #000;
    --HL-font-family: Menlo, Monaco, Consolas, Courier New, monospace;
    --HL-border-radius: 5px;
    --HL-padding-right: 4px;
    --HL-padding-left: 4px;
}






/* -------------------------------- THEME -------------------------------- */
/* -------------------------------- WHITE LABEL -------------------------------- */

.user-css-whitelabel {
   bottom: 50px;
   border-radius: var(--cont-rad);
   padding: var(--cont-pad-tiny);
   background: #00000099;
   box-shadow: var(--box-shadows);
   -moz-box-shadow: var(--box-shadows);
   -webkit-box-shadow: var(--box-shadows);
   opacity: .8;
   text-align: center;
}

.user-css-whitelabel small a {
    content: url(https://www.worldanvil.com/uploads/images/0f448d0499c3bc69c2852a37ad390f51.png);
    filter: drop-shadow(0 0 3px var(--aurora-1d));
    width: 100px;
    padding: 3px;
    display: block;
}


.user-css-whitelabel small, .user-css-whitelabel strong {
   display: block;
   font-size: 10pt;
}

.user-css-whitelabel small{
    color:white;
}
