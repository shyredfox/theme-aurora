/* ------------------ BOOTSTRAP CONTAINERS --------------- */

.user-css .text-danger, .user-css-extended .text-danger { color: var(--aurora-text-danger);}

.user-css .bg-primary {background: var(--aurora-primary); }
.user-css .bg-success { background: var(--aurora-success); }
.user-css .bg-info { background: var(--aurora-info); }
.user-css .bg-warning { background: var(--aurora-warning);} 
.user-css .bg-danger { background: var(--aurora-danger); }

.user-css .jumbotron {
    background: var(--aurora-3t);
	color: var(--aurora-text);
	border-radius: var(--cont-rad);
	font-family: var(--font-5);
}

.user-css .well {
    background: var(--aurora-4t);
    border: var(--border-tiny) solid var(--aurora-4d);
	 border-radius: var(--cont-rad);
}

.user-css .alert {
	font-family: var(--font-3); 
    color: var(--aurora-text);
    padding: var(--cont-pad-med);
    margin-bottom: var(--cont-pad);
    border: var(--border-thin) solid var(--aurora-wtb);
    border-radius: var(--cont-rad);
    background: none;
}

.user-css .alert-success { background: var(--aurora-success);}
.user-css .alert-info, .user-css-extended .alert-info {  background: var(--aurora-info);}
.user-css .alert-warning {  background: var(--aurora-warning);}
.user-css .alert-danger {  background: var(--aurora-danger); }


.user-css .btn, .user-css-extended .btn {
    color: var(--aurora-text);
	font-family: var(--font-3); 
    padding: var(--cont-pad-sm);
    margin-bottom: var(--cont-pad-sm);
    border: var(--border-thin) solid var(--aurora-wt) !important;
    border-radius: var(--cont-rad);
}

.user-css .label, .user-css-extended .label {
    color: var(--aurora-text);
	background: var(--aurora-4d);
	font-family: var(--font-3); 
    padding: var(--cont-pad-tiny);
    margin-bottom: var(--cont-pad-sm);
    border: var(--border-thin) solid var(--aurora-w) !important;
    border-radius: var(--cont-rad);
}

