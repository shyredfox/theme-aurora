	--characterLabelBg: var(--aurora-w); /* name and char box */
	--characterDatesColor: var(--aurora-night);

	
	/* main Char portrait */
 	--mainCharPortraitFrameBg: var(--highlightedCharGradient);
	--mainCharParentPortraitFrameBg: var(--inactiveCharGradient);
	
	/* relationship card */
	--characterRelationshipCardFont: var(--font-reg);
	--characterRelationshipCardBg: var(--aurora-night);
	--characterRelationshipCardColor: white;

	/* inactive char frames */
	--mainCharChildPortraitFrameBg: var(--inactiveCharGradient);
}

.user-css .rhea-family-tree .character-label, .user-css .rhea-family-tree .character-relationship {
	border-radius: var(--cont-rad);
}

.user-css [class*=rhea-search-contain] input {
	color: var(--aurora-night);
	border-radius: var(--cont-rad);
	border:  2px solid var(--aurora-wd);
	background: var(--aurora-w);
}

.user-css [class*=rhea-search-contain] input::placeholder {
	color: var(--aurora-wd);
}

