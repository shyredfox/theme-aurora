.table-aurora-icons .table tr td:nth-child(1), .table-aurora-icons .table tr td:nth-child(3) {
	text-align: center;
	max-width: 10%;
	min-width: 10%;
	font-size: var(--fsize-h3);
}
.table-aurora-icons .table tr td:nth-child(2), .table-aurora-icons .table tr td:nth-child(4)  {
    text-align: left;
	max-width: 40%;
	min-width: 40%;
	white-space: nowrap;
	font-family: var(--font-code);
}

/* Side box */
.side-box-vocab {
  opacity: 0.5;
  border-radius: 10px; 
  top: 100px; 
  right: 10px; 
  padding: 10px;
  font-size: .7em;
  line-height: 1.2em;
  border: var(--border-thin) solid var(--aurora-wtb); 
  background: var(--aurora-wl); 
  position: fixed;
  z-index:10;
}

.med-text  {
	background: var(--aurora-wt);
	border-radius: var(--cont-rad);
    display: inline-block;
    font-size: .8em;
    line-height: 1.5em;
	padding: var(--cont-pad);
}

.side-box-vocab a {color: var(--aurora-night);}
.side-box-vocab a:hover {color: var(--aurora-7l);}

@media only screen and (max-width: 1099px) {
    .side-box-vocab {display: none;}
}

.news-cols {
    margin: 15px;
    line-height: 1.5em;
    column-count: 2;
    column-gap: 3em;
    column-rule: 2px solid black;
}

.news-cols h2, .news-cols h3, .news-cols h4, .news-cols h5 {
    column-span: all;
    margin-top: 30px;
    margin-bottom: 20px;
}

/* fixes jump links leaveing the header off the page */
span, h2, h3, h4 {
  scroll-margin-top: 5rem; 
}